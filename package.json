{
    "name": "aem-fe-studio",
    "publisher": "shawnsandy",
    "displayName": "AEM Frontend Snippet Studio",
    "description": "Snippets for AEM frontend development",
    "version": "0.3.2",
    "repository": {
        "type": "git",
        "url": "https://github.com/shawn-sandy/aem-fe-studio.git"
    },
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Snippets"
    ],
    "scripts": {
        "build": "vsce package",
        "history": "auto-changelog --output HISTORY.md",
        "np:major": "np major --yolo=true --no-publish",
        "np:minor": "np minor --yolo=true --no-publish",
        "np:patch": "np patch --yolo=true --no-publish"
    },
    "contributes": {
        "snippets": [
            {
                "language": "java",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "plaintext",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "xml",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    },
    "devDependencies": {
        "auto-changelog": "^2.2.1",
        "np": "^6.5.0",
        "vsce": "^1.81.1"
    }
}
